<odoo>
    <data>


    <record model="ir.ui.view" id="taller_modelo_list">
      <field name="name">taller.modelo.list</field>
      <field name="model">taller.modelo</field>
      <field name="arch" type="xml">
        <tree name= "modeloaalist">
          <field name="modelo"/>
          <field name="brand"/>
          </tree>
      </field>
    </record>

    <record model="ir.ui.view" id="taller_modelo_form">
      <field name="name">taller.modelo.list</field>
      <field name="model">taller.modelo</field>
      <field name="arch" type="xml">
        <form name ="modeloaform">
          <sheet>
            <group name = "group_top">
              <group name ="group_left">
                <field name="modelo"/>
                <field name="brand"/>
              </group>
              <group name="group_right">
              </group>
            </group>
          </sheet>
        </form>
      </field>
    </record>




